omplexProtocol"]]},"references":{"doc://Attributes/documentation/Attributes/ComplexProtocol/groupValidation":{"identifier":"doc:\/\/Attributes\/documentation\/Attributes\/ComplexProtocol\/groupValidation","required":true,"fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"groupValidation","kind":"identifier"},{"text":": ","kind":"text"},{"text":"AnyValidator","kind":"typeIdentifier","preciseIdentifier":"s:10Attributes12AnyValidatorV"},{"text":"<","kind":"text"},{"text":"Attribute","kind":"typeIdentifier","preciseIdentifier":"s:10Attributes9AttributeO"},{"text":">","kind":"text"}],"type":"topic","role":"symbol","title":"groupValidation","abstract":[{"type":"text","text":"The validators for the attributes contained within this complex."}],"kind":"symbol","url":"\/documentation\/attributes\/complexprotocol\/groupvalidation"},"doc://Attributes/documentation/Attributes/Attributable":{"type":"topic","identifier":"doc:\/\/Attributes\/documentation\/Attributes\/Attributable","abstract":[{"type":"text","text":"This protocol provides the bare-minimum implementation for a group of attributes"},{"type":"text","text":" "},{"type":"text","text":"that can be validated, triggered, and displayed. An instance of this protocol"},{"type":"text","text":" "},{"type":"text","text":"represents the types of attributes accessible through this interface. Additionally,"},{"type":"text","text":" "},{"type":"text","text":"this protocol defines the validators required for each attribute and all the attributes"},{"type":"text","text":" "},{"type":"text","text":"as a whole. A similar requirement is placed on the triggers, where an instance may define a"},{"type":"text","text":" "},{"type":"text","text":"trigger that reacts to target attributes within this protocol."}],"title":"Attributable","role":"symbol","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"Attributable"}],"navigatorTitle":[{"kind":"identifier","text